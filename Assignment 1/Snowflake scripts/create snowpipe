CREATE OR REPLACE PIPE PIPE_PARQUET_AUTO
  AUTO_INGEST = TRUE
  AS
  COPY INTO user_name
   FROM (
  SELECT
    $1:column0::STRING AS FIRST_NAME,
    $1:column1::STRING AS LAST_NAME
  FROM @STG_S3_PARQUET (FILE_FORMAT => FF_PARQUET)
)

SHOW PIPES LIKE 'PIPE_PARQUET_AUTO';


SHOW PIPES LIKE 'PIPE_PARQUET_AUTO';

--From the output, copy the value in NOTIFICATION_CHANNEL (it will be an Amazon SQS ARN, for example arn:aws:sqs:region:account:queue-name). This is what S3 must send events to.

arn:aws:sqs:us-east-1:013001369188:sf-snowpipe-AIDAQGBXREZSNQAVTZ7NX-zcTeXA9-68Gk4Wff1ugaRQ

SELECT SYSTEM$PIPE_STATUS('PIPE_PARQUET_AUTO');
