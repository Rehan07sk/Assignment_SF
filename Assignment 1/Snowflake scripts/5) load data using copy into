LIST @STG_S3_PARQUET;

SELECT *
FROM @STG_S3_PARQUET (FILE_FORMAT => FF_PARQUET)
LIMIT ;

CREATE OR REPLACE TABLE user_name (
  FIRST_NAME STRING,
  LAST_NAME STRING
);

show file formats;

COPY INTO user_name (FIRST_NAME, LAST_NAME)
FROM (
  SELECT
    $1:column0::STRING AS FIRST_NAME,
    $1:column1::STRING AS LAST_NAME
  FROM @STG_S3_PARQUET (FILE_FORMAT => FF_PARQUET)
)
FILE_FORMAT = (FORMAT_NAME = FF_PARQUET);
